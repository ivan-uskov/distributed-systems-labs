extends layout

block content
  .mdl-card__title.mdl-card--expand.mdl-cell.mdl-cell--12-col
    table.mdl-data-table.mdl-js-data-table.mdl-shadow--2dp
      thead
        tr
          th.mdl-data-table__cell--non-numeric Id
          th Text
          th Status
        tbody
          for item in stats
            tr
              td.mdl-data-table__cell--non-numeric= item['id']
              td= item['text']
              td= item['type']